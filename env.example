BONITA_URL=http://localhost:8080/bonita
SECRET_KEY=super-secret-key-cambia-esto
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
# JSON con la definición de flujos semánticos y su correspondencia en Bonita.
# Ejemplo: {"vacaciones": {"process_name": "VacationRequest", "process_version": "1.0"}}
FLOW_DEFINITIONS={}

# Configuración de Base de Datos PostgreSQL
# Formato: postgresql://usuario:contraseña@host:puerto/nombre_bd
# Puede apuntar a una base de datos existente, las tablas se crearán en un esquema
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/bonita_bridge
# Nombre del esquema donde se crearán las tablas (por defecto: bdm)
# Si el esquema no existe, se creará automáticamente
DATABASE_SCHEMA=bdm
# Modo debug (True/False) - Activa logging de SQL queries
DEBUG=False


